{
  "name": "theme-default-mixed",
  "displayName": "%displayName%",
  "description": "%description%",
  "categories": [
    "Themes"
  ],
  "version": "1.0.0",
  "publisher": "vscode",
  "license": "MIT",
  "engines": {
    "vscode": "*"
  },
  "contributes": {
    "themes": [
      {
        "id": "Visual Studio Mix Dark",
        "label": "%darkMixColorThemeLabel%",
        "uiTheme": "vs-dark",
        "path": "./themes/dark_mix.json"
      },
      {
        "id": "Visual Studio Mix Light",
        "label": "%lightMixColorThemeLabel%",
        "uiTheme": "vs",
        "path": "./themes/light_mix.json"
      }
    ],
    "iconThemes": [
      {
        "id": "vs-minimal",
        "label": "%minimalIconThemeLabel%",
        "path": "./fileicons/vs_minimal-icon-theme.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/transtone/vscode-theme-mix"
  }
}
